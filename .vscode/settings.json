{
	//***************************************************//
	// "[osx]":
	// "editor.fontSize": 13.5,
	// "[linux]":
	"editor.fontSize": 13.6,
	"window.zoomLevel": 0.37,
	// "debug.console.fontSize": 20,
	"terminal.integrated.fontSize": 13,
	/*FOR BOTH*/
	"makefile.makePath": "./Makefile",
	"makefile.showOnStartup": false,
	"terminal.integrated.shell.linux": "/bin/bash",
	"terminal.integrated.defaultProfile.linux": "bash",
	"workbench.startupEditor": "none",
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"doxdocgen.generic.useGitUserEmail": true,
	"doxdocgen.generic.useGitUserName": true,
	"doxdocgen.c.triggerSequence": "/**",
	//************************************************//
	"compiler-explorer.url": "https://godbolt.org", // url of the compiler explorer you want to use.
	"compiler-explorer.compiler": "armg454", // Compiler code - See help below.
	"compiler-explorer.options": [
		"-O3"
	], // Compiler options
	"compiler-explorer.debug": true,
	"compiler-explorer.include": [
		// "${workspaceFolder}/CPP_01/ex04/src/${workspaceFolder}/CPP_01/ex04/src/main.cpp",
		//	"${workspaceFolder}/CPP_00/ex01/src/${workspaceFolder}/CPP_00/ex01/src/my_awesome_phonebook.cpp"
	],
	"window.customTitleBarVisibility": "auto",
	"rewrap.wrappingColumn": 70,
	"files.associations": {
		"array": "cpp",
		"atomic": "cpp",
		"bit": "cpp",
		"*.tcc": "cpp",
		"cctype": "cpp",
		"clocale": "cpp",
		"cmath": "cpp",
		"compare": "cpp",
		"concepts": "cpp",
		"cstdarg": "cpp",
		"cstddef": "cpp",
		"cstdint": "cpp",
		"cstdio": "cpp",
		"cstdlib": "cpp",
		"cstring": "cpp",
		"cwchar": "cpp",
		"cwctype": "cpp",
		"deque": "cpp",
		"unordered_map": "cpp",
		"vector": "cpp",
		"exception": "cpp",
		"algorithm": "cpp",
		"functional": "cpp",
		"iterator": "cpp",
		"memory": "cpp",
		"memory_resource": "cpp",
		"numeric": "cpp",
		"optional": "cpp",
		"random": "cpp",
		"string": "cpp",
		"string_view": "cpp",
		"system_error": "cpp",
		"tuple": "cpp",
		"type_traits": "cpp",
		"utility": "cpp",
		"initializer_list": "cpp",
		"iosfwd": "cpp",
		"iostream": "cpp",
		"istream": "cpp",
		"limits": "cpp",
		"new": "cpp",
		"numbers": "cpp",
		"ostream": "cpp",
		"ranges": "cpp",
		"span": "cpp",
		"stdexcept": "cpp",
		"streambuf": "cpp",
		"cinttypes": "cpp",
		"typeinfo": "cpp",
		"ctime": "cpp",
		"iomanip": "cpp",
		"sstream": "cpp",
		"fstream": "cpp",
		"bitset": "cpp",
		"chrono": "cpp",
		"ratio": "cpp"
	},
	"C_Cpp.dimInactiveRegions": false,
	"doxdocgen.generic.commandSuggestionAddPrefix": true, // true indicates that the extension will print debug to its output channel.
	"files.exclude":
	{
		"**/CPP_00": true,
		"**/CPP_01": true,
		"**/CPP_02": true,
		"**/CPP_03": true,
	},
	"github.copilot.editor.enableAutoCompletions": false,
	"pdf-preview.default.sidebar": false
}